<style type="text/css">
    .normal-body{
        width: 105%;
        height: 42px;
        background: #fff;
        padding: 3px;
        text-align: center;
        box-shadow: 0 2px 5px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12);
        font-family: 'Open Sans Light', sans-serif;
        margin-bottom: 2%;
        position: relative;
    }
    .normal-index{
        width:20%;
        height: 20px;
        float: right;
        text-align:center;
        font-size: inherit;
        margin-top: 1%;
        position: relative;


    }
    .normal-index1{
        width:188px;
        height: 50px;
        text-align: left;
        padding-left: 2%;
    }
    .page-index-mobile{
        width: 96%;
        background: #fff;
        padding: 3px;
        text-align: center;
        box-shadow: 0 2px 5px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12);
        font-family: 'Open Sans Light', sans-serif;
        margin-bottom: 2%;
        position: relative;
        left: 10px;
        margin-top: 1%;
    }
</style>

<div class="edit-list">
  <div data-role="content" data-theme="c">
    <ul data-role="listview" data-inset="true">
      <li data-role="list-divider" style="text-align: center">
        Your Branching With Other Tree
      </li>

      <% for page in current_user.branches %>
          <div class="page-index-mobile">
            <div class="normal-index">
              <%= link_to '<i class="glyphicon glyphicon-tree-deciduous">Out</i>'.html_safe, looking_branch_path(page), method: "post", :class=>"btn btn-success " , :style => " text-decoration:none;color:#000",:rel=>"external" %>

            </div>
            <div class="normal-index1">
              <div>
                <% if page.page.image.present? %>
                    <%=link_to image_tag(page.page.image,:width=>"46px;",:height=>"47px;"), page.page  %>
                <% else %>
                    <%=link_to  image_tag("tree.png",:width=>"46px;",:height=>"47px;",),page.page  %>
                <% end %>
              </div>
              <p style="position: relative;bottom: 40px;left: 50px;font-size: 80%"><b><%= page.page.name %></b>
                <br>
                <i class="glyphicon glyphicon-leaf" style="color: forestgreen"></i> <%= page.page.branches.count %>
              </p>
            </div>
          </div>
          <% end %>
      <li data-role="list-divider" style="text-align: center"><br></li>
    </ul>
  </div>

</div>

